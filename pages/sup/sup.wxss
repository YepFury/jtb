page {
    height: 100%;
    background-image: url(http://jtb-1253661417.cossh.myqcloud.com/images/%E4%BA%8B%E6%95%85%E5%A4%84%E7%90%86/bg.png);
    background-repeat: no-repeat;
    background-size: contain;
}
.sup {
    width: 200rpx;
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
    border-radius: 50%;
    background: rgba(255, 68, 0, .8);  
    color: #fff;
    font-size: 28rpx;
    margin-left: 500rpx;
    margin-top: 50rpx;
}
.info {
    text-align: center;
    font-size: 28rpx;
    
    margin-top: 40rpx;
}
.info text {
    height: 60rpx;    
    line-height: 60rpx;
}
.info button {
    width: 30%;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #17b3ef;
    font-size: 38rpx;
    color: #fff;
    margin-left: 35%;
    margin-top: 20rpx;
}
.tips {
    margin-top: 200rpx;
    font-size: 24rpx;
    width: 70%;
    margin-left: 15%;
}
.tips text {
    display: block;
    width: 100%;
    line-height: 36rpx;
}
.title {
    color: #17b3ef;
}
.support {
    width: 200rpx;
    height: 200rpx;
    line-height: 200rpx;
    border-radius: 50%;
    color: #efefef;
    font-size: 34rpx;
    position: fixed;
    bottom: 60rpx;
    left: 50%;
    transform: translateX(-50%);
    background-color: #ccc;
}